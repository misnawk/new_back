{
  "version": 2,
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "bakend/public/api-docs"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/main.js"
    }
  ]
}
